{
  "configurations": [
    {
      "name": "C++ Launch",
      "type": "cppvsdbg",
      "request": "attach",
      "processId": "${command:pickProcess}",
      "symbolSearchPath": "${workspaceFolder}/Debug",
      "requireExactSource": true,
      "logging": {
        "moduleLoad": true,
        "trace": true
      }
    }
  ],
  "version": "2.0.0"
}